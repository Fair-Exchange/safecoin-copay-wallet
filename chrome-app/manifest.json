  "manifest_version": 2,
  "name": "safecoin",
  "description": "A Secure SafeCoin Wallet",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "videoCapture",
    "webview"
